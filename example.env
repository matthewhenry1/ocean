# Public key for the Solana wallet
PUBLIC_KEY=your_public_key_here

# Private key for the Solana wallet (in base58 format)
PRIVATE_KEY=your_private_key_here

# API token for Bitquery
BITQUERY_OAUTH_TOKEN=your_bitquery_oauth_token_here

# Solana network URL (optional, defaults to mainnet-beta)
SOLANA_NETWORK=https://api.mainnet-beta.solana.com

# Jupiter API base URL (optional, defaults to https://quote-api.jup.ag/v6)
JUPITER_API_BASE_URL=https://quote-api.jup.ag/v6

# Maximum retries for token pair retrieval
MAX_RETRIES=5